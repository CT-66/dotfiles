#!/bin/sh
export XDG_SESSION_TYPE=x11
xrdb -merge $HOME/Dotfiles/X11/Xresources && sleep 0.0001
xmodmap $HOME/Dotfiles/X11/Xmodmap && sleep 0.0001

exec i3
# exec bspwm
# exec xmonad
# exec qtile start
# exec awesome

# exec openbox-session

case $1 in

  i3)
  exec i3
  ;;
  bspwm)
  exec bspwm
  ;;
  qtile)
  exec qtile
  ;;
  xmonad)
  exec xmonad
  ;;
  awesome)
  exec awesome
  ;;
  openbox)
  exec openbox-session
  ;;
esac
